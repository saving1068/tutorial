  align-items: center;
  padding: 20rpx;
  box-sizing: border-box;

}
.look-item{
   width: 300rpx; 
   height: 300rpx;
  
   text-align: center;
} 
.prodict{
  padding: 20rpx 20rpx 0;
  box-sizing: border-box;
  display: flex;
  justify-content: space-around;
  align-items: center;
  flex-wrap: wrap;
  background: rgb(240, 240, 240);
  }
  .prodict:after {
content: "";
flex: auto;
}


.prodict-item{
  width: 340rpx;
  text-align: center;
  margin-bottom: 20rpx;
  background: #fff;
  border-radius: 10rpx;
  padding: 20rpx;
  box-sizing: border-box;
}
.item-image{
  height: 240rpx;
  margin-top: 20rpx;
 
